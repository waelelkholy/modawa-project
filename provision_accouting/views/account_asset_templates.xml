<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="assets_backend" name="account assets" inherit_id="web.assets_backend">
		<xpath expr="." position="inside">
			<link rel="stylesheet" type="text/scss" href="/provision_accouting/static/src/scss/account_asset.scss" />
			<script type="text/javascript" src="/provision_accouting/static/src/js/account_asset.js" />
		</xpath>
	</template>
	<template id="qunit_suite" name="account_asset_tests" inherit_id="web.qunit_suite">
		<xpath expr="//t[@t-set='head']" position="inside">
			<script type="text/javascript" src="/provision_accouting/static/tests/account_asset_tests.js" />
		</xpath>
	</template>
</odoo>